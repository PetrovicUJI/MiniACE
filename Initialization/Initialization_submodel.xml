			<functions>
				<function>
					<name>Firm_initialization</name>
					<description></description>
					<currentState>Start_Firm</currentState>
					<nextState>Firm_Info</nextState>
					<condition>
						<lhs>
							<value>a.initialization</value>
						</lhs>
						<op>EQ</op>
						<rhs>
							<value>1</value>
						</rhs>
					</condition>
